{
  "tsserver.npm": "/usr/local/bin/npm",
  "eslint.probe": [
    "javascript",
    "typescript",
    "typescriptreact",
    "javascriptreact",
    "typescript.tsx"
  ],
  "codeLens.enable": true,
  "codeLens.separator": "",
  "diagnostic.errorSign": "✘",
  "diagnostic.warningSign": "●",
  "diagnostic.infoSign": "●",
  "diagnostic.hintSign": "►",
  "diagnostic.messageTarget": "float",
  "diagnostic.messageDelay": 300,
  "diagnostic.virtualText": true,
  "diagnostic.virtualTextPrefix": "",
  "diagnostic.virtualTextCurrentLineOnly": true,
  "diagnostic.maxWindowWidth": 40,
  "diagnostic.maxWindowHeight": 6,
  "diagnostic.format": "[%source] %message [%severity]",
  "coc.preferences.currentFunctionSymbolAutoUpdate": true,
  "coc.preferences.enableMarkdown": true,
  "javascript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyBraces": true,
  "prettier.eslintIntegration": true,
  "eslint.packageManager": "yarn",
  "latex.lint.onChange": true
}
